<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/view_choose_energy_for_uac_fragment_slider"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <android.support.v4.view.ViewPager
        android:id="@+id/view_uac_fragment_slider_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
   
    <fragment
        android:id="@+id/frag_energy_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.rdupuis.amikcal.energy.Frag_EnergyList" />
    
    <fragment
        android:id="@+id/frag_energy_list2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.rdupuis.amikcal.energy.Frag_EnergyList" />
    </android.support.v4.view.ViewPager>

</LinearLayout>